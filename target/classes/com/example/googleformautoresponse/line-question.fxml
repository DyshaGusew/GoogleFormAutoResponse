<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="line" alignment="CENTER" style="-fx-background-color: red;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextField fx:id="entry" alignment="CENTER" prefHeight="30.0" prefWidth="116.0" promptText="Entry значение">
            <font>
                <Font size="14.0" />
            </font>
            <HBox.margin>
                <Insets right="20.0" />
            </HBox.margin>
        </TextField>
        <TextField fx:id="frequency" alignment="CENTER" layoutX="30.0" layoutY="16.0" prefHeight="30.0" prefWidth="196.0" promptText="Частоты ответов">
            <font>
                <Font size="14.0" />
            </font>
            <HBox.margin>
                <Insets right="20.0" />
            </HBox.margin>
        </TextField>
        <TextField fx:id="answer" alignment="CENTER" layoutX="172.0" layoutY="16.0" prefHeight="30.0" prefWidth="170.0" promptText="Ответы">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</HBox>
